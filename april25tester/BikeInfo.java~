public class BikeInfo { 
  private String[] data; 
  
  public BikeInfo(String s) { 
    data = s.split(","); 
  } 
  
  public int getTripTime() { 
    return Integer.parseInt(data[5]); 
  } 
  
  public boolean isSubscriber() { 
    return data[10].equals("Subscriber"); 
  } 
  
  public boolean isFemale() { 
    return data[11].equals("Female"); 
  } 
  
  public int dOfB() { 
    return Integer.parseInt(data[12]); 
  } 
  
  public int iD() { 
    return Integer.parseInt(data[4]); 
  } 
  
}