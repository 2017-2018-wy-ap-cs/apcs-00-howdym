import java.util.*;

public class insSort { 
  public insSort() { 
    // do nothing 
  } 
  
  public void addStuff(ArrayList<Integer> x, int y, int pivot) { 
    x.add(y, x.get(pivot)); 
    x.remove(pivot); 
  } 
  
